header
    float-w(100%)
    height 700px
    background colH
    position relative
    

    
    h1
        color #fff
        margin-bottom 25px
    
    .content
        height 100%;
        
    article
        width 690px
    p 
        color #fff !important
        
.h__floating-t
    abs(table,100%,100%,0px,0px)
    padding-top 96px
        
    
.h__img-bg
    abs(block,100%,100%,0px,0px)
    bgP(top,center,cover)
    opacity .3
    background url(../img/home.jpg)
    
    
.h__company
    margin-top 65px
    float-w(100%)
    
.h__company__item
    float left
    display inline-block
    padding-right 38px
    
    img 
        float left
        width auto
        max-width 100%
        display block
        
.h__company__item:last-child
    padding-right 0px
    
    
.h__form
    float right
    width 400px
    height 480px
    background colbg
    border-radius 5px
    

    
    
